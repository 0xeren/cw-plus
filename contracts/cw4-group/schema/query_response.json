{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryResponse",
  "description": "This type is never used in code, just for the JSON Schema export to allow tooling to easily map the response types to the original query variant.",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "admin"
      ],
      "properties": {
        "admin": {
          "$ref": "#/definitions/AdminResponse"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "total_weight"
      ],
      "properties": {
        "total_weight": {
          "$ref": "#/definitions/TotalWeightResponse"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "list_members"
      ],
      "properties": {
        "list_members": {
          "$ref": "#/definitions/MemberListResponse"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "member"
      ],
      "properties": {
        "member": {
          "$ref": "#/definitions/MemberResponse"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "hooks"
      ],
      "properties": {
        "hooks": {
          "$ref": "#/definitions/HooksResponse"
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "AdminResponse": {
      "type": "object",
      "properties": {
        "admin": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "additionalProperties": false
    },
    "HooksResponse": {
      "type": "object",
      "required": [
        "hooks"
      ],
      "properties": {
        "hooks": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "additionalProperties": false
    },
    "Member": {
      "description": "A group member has a weight associated with them. This may all be equal, or may have meaning in the app that makes use of the group (eg. voting power)",
      "type": "object",
      "required": [
        "addr",
        "weight"
      ],
      "properties": {
        "addr": {
          "type": "string"
        },
        "weight": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      },
      "additionalProperties": false
    },
    "MemberListResponse": {
      "type": "object",
      "required": [
        "members"
      ],
      "properties": {
        "members": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Member"
          }
        }
      },
      "additionalProperties": false
    },
    "MemberResponse": {
      "type": "object",
      "properties": {
        "weight": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        }
      },
      "additionalProperties": false
    },
    "TotalWeightResponse": {
      "type": "object",
      "required": [
        "weight"
      ],
      "properties": {
        "weight": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      },
      "additionalProperties": false
    }
  }
}
